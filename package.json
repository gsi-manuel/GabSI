{
  "name": "ts-nodejs-express-webpack",
  "version": "0.1.0",
  "description": "Project node-express ",
  "main": "./dist/index.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "build": "webpack --mode production --config config/webpack.config.js",
    "start": "yarn run build && node ./dist/index.js"
  },
  "keywords": [
    "typescript.",
    "webpack",
    "nodejs",
    "express"
  ],
  "author": "gsi-manuel",
  "license": "MIT",
  "dependencies": {
    "body-parser": "^1.19.0",
    "cors": "^2.8.5",
    "express": "^4.17.1",
    "helmet": "^3.19.0",
    "husky": "^3.0.1",
    "lint-staged": "^9.2.0",
    "mongoose": "^5.6.5",
    "prettier": "^1.18.2",
    "q": "^1.5.1",
    "ts-loader": "^6.0.4",
    "ts-node": "^8.3.0",
    "tslint": "^5.18.0",
    "typegoose": "^5.8.1",
    "typescript": "^3.5.3",
    "webpack": "^4.36.1",
    "webpack-cli": "^3.3.6"
  },
  "devDependencies": {
    "@types/body-parser": "^1.17.0",
    "@types/cors": "^2.8.5",
    "@types/express": "^4.17.0",
    "@types/helmet": "^0.0.43",
    "@types/mongoose": "^5.5.9",
    "@types/node": "^12.6.8",
    "@types/webpack": "^4.4.0",
    "dotenv": "^8.0.0",
    "mongoose-middleware": "^1.0.0",
    "reflect-metadata": "^0.1.13"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "src/**/*.{js,ts,json,md}": [
      "prettier --write --print-width 120 --single-quote --tab-width 2 --trailing-comma all --no-bracket-spacing",
      "git add"
    ]
  }
}
